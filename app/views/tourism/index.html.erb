<p id="notice"><%= notice %></p>

<h1>Tourism</h1>

<table>
  <thead>
    <tr>
      <th>Firstname</th>
      <th>Lastname</th>
      <th>Email</th>
      <th>Mobile</th>
      <th>City</th>
      <th>Tourist place</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tourist.each do |tourist| %>
      <tr>
        <td><%= tourist.firstname %></td>
        <td><%= tourist.lastname %></td>
        <td><%= tourist.email %></td>
        <td><%= tourist.mobile %></td>
        <td><%= tourist.city %></td>
        <td><%= tourist.tourist_place %></td>
         <td><%= link_to 'Edit', edit_tourism_path(tourist) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Registration', tourism_new_path %>
